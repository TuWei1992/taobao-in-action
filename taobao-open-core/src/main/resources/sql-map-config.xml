<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
    <settings enhancementEnabled="true" maxTransactions="20" useStatementNamespaces="true"
        maxRequests="32" maxSessions="10"/>

    <!-- Identify all SQL Map XML files to be loaded by this SQL map. Relative to classpath -->
    <sqlMap resource="com/dream/recommend/model/sqlmap/RecommendScopeSettingSQL.xml"/>
    <sqlMap resource="com/dream/recommend/model/sqlmap/RecommendModeSettingSQL.xml"/>
    <sqlMap resource="com/dream/menu/model/sqlmap/MenuSQL.xml"/>
    <sqlMap resource="com/dream/common/model/sqlmap/SysParamSQL.xml"/>
    <sqlMap resource="com/dream/item/model/sqlmap/ItemSQL.xml"/>
    <sqlMap resource="com/dream/auth/model/sqlmap/AuthSQL.xml"/>
    <sqlMap resource="com/dream/shop/model/sqlmap/ShopSQL.xml"/>
</sqlMapConfig>